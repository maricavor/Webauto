<div class="row-fluid">
  <div class="span12">
      <div class="page-header">
      <h2><%= t("signin.sign_in") %></h2>
    </div>
    </div>
    </div>
<div class="row-fluid">
  <div class="span6 well">
  <%= vehicle_form_for(resource, :as => resource_name, :url => session_path(resource_name), :html=>{:class=>"form-vertical",:remote=>false}) do |f| %>

  <%= f.text_field :email, :autofocus => true,:label=>t("signin.email") %>
  <%= f.text_field :password,:label=>t("signin.password") %>
  <%= f.check_box :remember_me,label: t("signin.remember_me") if devise_mapping.rememberable? %>
 <div style="padding-top:20px;">
      <%= f.submit t("signin.sign_in_btn"),:class=>"btn btn-primary" %>
      </div>
 
<% end %>
<%= render "devise/shared/links" %>
</div>
  <div class="span6 text-center">
  <br /><br />
 <h3><%= t("signin.new_to")  %></h3>
 <p><%= t("signin.get_started") %></p>
 <%= link_to t("navigation.register"), new_registration_path(resource_name),:class => 'btn btn-primary' %>
  </div>
  </div>