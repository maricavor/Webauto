<% content_for :title,@title %>
<div class="row-fluid">
  <div class="span3">
    <%= render 'users/well',:active=>"garage" %>
  </div>
  <div class="span9">
    <div class="page-header">
      <h2>My Garage</h2>
    </div>
    <% if @vehicles %>
      <div class="paginator">
        <%= render 'vehicles/partials/paginator' %>
      </div>
      <div class="row-fluid">
        <div class="span6">
          <div class="page_entries_info">
            <%= render 'vehicles/partials/page_entries_info' %>
          </div>
        </div>
        <div class="span2">
          <div class="preloader"></div>
        </div>
        <div class="span4">
       <div class="text-right hidden-phone">
            <%= link_to '<i class="icon-plus icon-white"></i> Add a Car'.html_safe, new_garage_item_path,:class=> "btn btn-info" %>
          </div>
         <div class="text-left visible-phone">
            <%= link_to '<i class="icon-plus icon-white"></i> Add a Car'.html_safe,new_garage_item_path,:class=> "btn btn-info" %>
          </div>
        </div>
      </div>
      <div id="my_vehicles">
        <%= render 'garage_items/vehicles',vehicles: @vehicles %>
      </div>
      <% if @vehicles.count>5 %>
        <div class="page_entries_info">
          <%= render 'vehicles/partials/page_entries_info' %>
        </div>
        <div class="paginator">
          <%= render 'vehicles/partials/paginator' %>
        </div>
      <% end %>
    <% else %>
    
          <div>
       
            <%= link_to '<i class="icon-plus icon-white"></i> Add a Car'.html_safe,new_garage_item_path,:class=> "btn btn-info" %>
            </div>
    <% end %>
  </div>
</div>
